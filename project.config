[project]
	description = The Leek group guide to data sharing 
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/Illu (IlluYing)
[access "refs/for/refs/*"]
	read = group user/Illu (IlluYing)
	push = group user/Illu (IlluYing)
	label-Code-Review = -2..+2 group user/Illu (IlluYing)
	label-Verified = -1..+1 group user/Illu (IlluYing)
	rebase = group user/Illu (IlluYing)
	removeReviewer = group user/Illu (IlluYing)
	submit = group user/Illu (IlluYing)
[access "refs/heads/*"]
	read = group user/Illu (IlluYing)
	create = group user/Illu (IlluYing)
	push = +force group user/Illu (IlluYing)
	pushMerge = group user/Illu (IlluYing)
[access "refs/tags/*"]
	push = +force group user/Illu (IlluYing)
	pushTag = group user/Illu (IlluYing)
	pushSignedTag = group user/Illu (IlluYing)
